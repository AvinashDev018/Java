import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int team=a/b;
        int left=a%b;
        System.out.println("Total teams formed: "+team);
        System.out.println("Players left out: "+left);
    }
}
